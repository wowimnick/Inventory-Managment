            <padding>
                <Insets left="5.0" />
            </padding>
        </Label>
        <Label fx:id="partError" textFill="RED" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="9">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <TextField fx:id="partTypeText" GridPane.columnIndex="1" GridPane.rowIndex="7" />
        <Label fx:id="machineidORCompany" text="Machine ID" GridPane.halignment="CENTER" GridPane.rowIndex="7">

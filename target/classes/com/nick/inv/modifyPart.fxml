        <Label text="ADD PART" GridPane.halignment="CENTER">
            <font>
                <Font name="Calibri Bold" size="13.0" />
            </font>
        </Label>
        <RadioButton fx:id="inHouse" mnemonicParsing="false" onAction="#partType" text="In-House" GridPane.columnIndex="1">
            <toggleGroup>
                <ToggleGroup fx:id="partType" />
            </toggleGroup>
        </RadioButton>
        <RadioButton fx:id="outSourced" mnemonicParsing="false" onAction="#partType" prefHeight="18.0" prefWidth="95.0" text="OutSourced" toggleGroup="$partType" GridPane.columnIndex="2" />
        <Label text="Part ID" GridPane.halignment="CENTER" GridPane.rowIndex="2">
            <font>
                <Font name="System Bold Italic" size="10.0" />
            </font>
        </Label>
        <TextField fx:id="selPartID" disable="true" promptText="Autogenerated" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <font>
                <Font size="11.0" />
            </font>
        </TextField>
        <Label text="Name" GridPane.halignment="CENTER" GridPane.rowIndex="3">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="partName" GridPane.columnIndex="1" GridPane.rowIndex="3" />
